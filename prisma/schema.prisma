generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

model DonorInformation {
  id                       String       @id @default(cuid())
  lastName                 String       @map("Last Name (2)")     
  firstName                String       @map("First Name (3)")      
  middleName               String?      @map("Middle Name (4)")
  age                      Int          @map("AGE (5)")        
  sex                      String       @map("SEX (6)") 
} 
